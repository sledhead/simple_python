
#simple python file
print('the program is starting up')
print('Please enter user name: ')
user_input = input()

print('last name: ')
last_name = input()

print(user_input)
print(last_name)

print('next section')

name_dict = {}
name_dict['first_name'] = user_input
name_dict['last_name'] = last_name

print('hello {first_name}, we are glad to have you Mr. {last_name}. '.format(**name_dict) )

